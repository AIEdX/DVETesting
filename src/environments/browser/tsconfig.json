{
  "references": [{ "path": "../../core" }],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "../",
    "rootDirs": [],
    "types": ["babylonjs"],
    "outDir": "../../../compiled/raw/environments",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@data/*": ["../../core/data/*"],
      "@clientData/*": ["../../core/data/client/*"],
      "@sharedData/*": ["../../core/data/shared/*"],
      "@core/*": ["../../core/*"],
      "@dve/*": ["../../core/libs/DVE/*"],
      "@dbt/*": ["../../core/libs/DVE/Libs/DivineBinaryTags/*"]
    },
    "plugins": [
      { "transform": "typescript-transform-paths" }
    ],
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
